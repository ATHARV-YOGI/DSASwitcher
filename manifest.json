{
  "manifest_version": 3,
  "name": "DSASwitcher",
  "version": "1.0",
  "description": "Redirects to DSA questions on page load from curated sheets.",
  "permissions": ["storage", "tabs", "webNavigation"],
  "background": {
    "service_worker": "redirector.js"
  },
  "options_page": "options.html",
  "action": {
    "default_popup": "options.html",
    "default_icon": "icons/icon.png"
  },
  "icons": {
    "128": "icons/icon.png"
  }
}
